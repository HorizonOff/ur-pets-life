%div
  .page-title
    .title_left
      %h3 Edit Item

  .clearfix

  = form_for @item, url: admin_panel_item_path(@item),
  html: { multipart: true, method: :put, class: 'form-horizontal form-label-left with_location', autocomplete: 'off'} do |f|

    = render partial: 'form', locals: { f: f }

    = f.submit 'Update Item', class: 'btn btn-success pull-right'
