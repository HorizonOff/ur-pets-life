wb = xlsx_package.workbook

wb.add_worksheet(name: "Orders") do |sheet|
  sheet.add_row %w(Invoice\ Date Invoice\ No Client\ Name Jurisdiction\ in\ which\ service\ was\ provided Total\
                   Inclusive\ of\ VAT)
  @tax_reports.each do |report|
    invoice_no = 'INV-' + report.id.to_s
    customer_name = report.user.first_name + ' ' + report.user.last_name
    sheet.add_row [report.delivery_date, invoice_no, customer_name, report.location.city]
  end
end
