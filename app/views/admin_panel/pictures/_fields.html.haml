.nested-fields
  .form-group
    .col-md-9.col-sm-9.col-xs-12
      -if f.object.attachment?
        = image_tag f.object.attachment.thumb.url, class: 'avatar-show', style: ' display:block;margin:auto'
      = f.hidden_field :attachment_cache
    = link_to_remove_association "Remove", f, { class: 'btn btn-danger pull-right'}
