%div
  .page-title
    .title_left
      %h3 New vet 

  .clearfix
  
  = form_for @vet, url: admin_panel_vets_path,
  html: { multipart: true, method: :post, class: 'form-horizontal form-label-left', autocomplete: 'off'} do |f|

    = render partial: 'form', locals: { f: f }
    
    = f.submit 'Create vet', class: 'btn btn-success pull-right'

:javascript
  init_vet_map();
