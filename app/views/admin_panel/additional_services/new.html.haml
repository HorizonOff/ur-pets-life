%div
  .page-title
    .title_left
      %h3 New Additional service

  .clearfix
  
  = form_for @additional_service, url: admin_panel_additional_services_path,
  html: { multipart: true, method: :post, class: 'form-horizontal form-label-left with_location', autocomplete: 'off'} do |f|

    = render partial: 'form', locals: { f: f }
    
    = f.submit 'Create additional service', class: 'btn btn-success pull-right'

:javascript
  init_map();
