%tr
  %th{ scope: 'row' } #{user.id}
  %td #{user.name}
  %td #{user.email}
  %td #{user.mobile_number}
  %td 
  %td
    =link_to 'Show', admin_panel_user_path(user), class: 'btn btn-primary btn-xs'
    =link_to 'Edit', edit_admin_panel_user_path(user), class: 'btn btn-warning btn-xs'
    =link_to 'Delete', admin_panel_user_path(user), method: :delete, class: 'btn btn-danger btn-xs'
