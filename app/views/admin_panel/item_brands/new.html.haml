%div
  .page-title
    .title_left
      %h3 Add New Brand

  .clearfix

  = form_for @admin_panel_item_brand, url: admin_panel_item_brands_path,
  html: { multipart: true, method: :post, class: 'form-horizontal form-label-left with_location', autocomplete: 'off'} do |f|

    = render partial: 'form', locals: { f: f }
    .clearfix
    .row
      = f.submit 'Create Brand', class: 'btn btn-success col-md-offset-2'
